<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleDevelopmentRegion</key>
	<string>$(DEVELOPMENT_LANGUAGE)</string>
	<key>CFBundleDisplayName</key>
	<string>NaviSafe</string>
	<key>CFBundleExecutable</key>
	<string>$(EXECUTABLE_NAME)</string>
	<key>CFBundleIdentifier</key>
	<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleName</key>
	<string>NaviSafe</string>
	<key>CFBundlePackageType</key>
	<string>APPL</string>
	<key>CFBundleShortVersionString</key>
	<string>$(FLUTTER_BUILD_NAME)</string>
	<key>CFBundleSignature</key>
	<string>????</string>
	<key>CFBundleVersion</key>
	<string>$(FLUTTER_BUILD_NUMBER)</string>
	<key>LSRequiresIPhoneOS</key>
	<true/>
	
	<!-- Location permissions for GNSS positioning -->
	<key>NSLocationWhenInUseUsageDescription</key>
	<string>NaviSafe requires location access to provide precise navigation and collision detection for autonomous vehicle safety.</string>
	<key>NSLocationAlwaysAndWhenInUseUsageDescription</key>
	<string>NaviSafe requires location access to provide continuous navigation and collision detection for autonomous vehicle safety.</string>
	<key>NSLocationAlwaysUsageDescription</key>
	<string>NaviSafe requires location access to provide continuous navigation and collision detection for autonomous vehicle safety.</string>
	
	<!-- Bluetooth permissions for mesh networking -->
	<key>NSBluetoothAlwaysUsageDescription</key>
	<string>NaviSafe uses Bluetooth to create a mesh network for cooperative positioning and enhanced navigation accuracy.</string>
	<key>NSBluetoothPeripheralUsageDescription</key>
	<string>NaviSafe uses Bluetooth to communicate with other vehicles for cooperative navigation and collision avoidance.</string>
	
	<!-- Motion and fitness permissions for collision detection -->
	<key>NSMotionUsageDescription</key>
	<string>NaviSafe uses motion sensors to detect potential collisions and provide safety alerts.</string>
	
	<!-- Camera permissions for future AR features -->
	<key>NSCameraUsageDescription</key>
	<string>NaviSafe may use the camera for augmented reality navigation features.</string>
	
	<!-- Microphone permissions for voice alerts -->
	<key>NSMicrophoneUsageDescription</key>
	<string>NaviSafe may use the microphone for voice command navigation features.</string>
	<key>UILaunchStoryboardName</key>
	<string>LaunchScreen</string>
	<key>UIMainStoryboardFile</key>
	<string>Main</string>
	<key>UISupportedInterfaceOrientations</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>UISupportedInterfaceOrientations~ipad</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationPortraitUpsideDown</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>CADisableMinimumFrameDurationOnPhone</key>
	<true/>
	<key>UIApplicationSupportsIndirectInputEvents</key>
	<true/>
</dict>
</plist>
